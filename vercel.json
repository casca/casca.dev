{
  "version": 2,
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    {
      "source": "/goto",
      "destination": "https://goto.casca.vercel.app"
    },
    {
      "source": "/goto/(.*)",
      "destination": "https://goto.casca.vercel.app/$1"
    }
  ],
  "redirects": [
    {
      "source": "/goto/(?<slug>(?!favicon\\.ico)[^/]+)",
      "permanent": false,
      "destination": "https://goto.casca.vercel.app/api/redirect/$slug"
    }
  ]
}

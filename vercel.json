{
  "version": 2,
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    {
      "source": "/((goto|covid)(?!/api)(?:/.*)?)",
      "destination": "https://$2.casca.vercel.app/$1"
    }
  ],
  "redirects": [
    {
      "source": "/goto/api/(.*)",
      "destination": "https://goto.casca.vercel.app/api/$1"
    },
    {
      "source": "/goto/((?!favicon)[^/]+)",
      "destination": "https://goto.casca.vercel.app/api/redirect/$1"
    },
    {
      "source": "/(cms|admin)",
      "destination": "/admin/cms"
    },
    {
      "source": "/repo/(.*)",
      "destination": "https://github.com/casca/$1"
    }
  ]
}

{
  "version": 2,
  "routes": [
    {
      "src": "/goto(.*)/(?:index\\.html?)?",
      "status": 200,
      "headers": { "Location": "/goto$1" }
    },
    {
      "src": "/goto/(.*).htm(?:l?)",
      "status": 200,
      "headers": { "Location": "/goto/$1" }
    },
    { "src": "/goto", "methods": ["GET"], "dest": "https://goto.casca.vercel.app/", "status": 200 },
    {
      "src": "/goto",
      "methods": ["POST"],
      "status": 307,
      "headers": { "Location": "https://goto.casca.vercel.app/api/date" }
    },
    {
      "src": "/goto/(?!favicon\\.ico/?)(?<slug>[^/]+)",
      "status": 302,
      "headers": { "Location": "https://goto.casca.vercel.app/api/redirect/$slug" }
    },
    {
      "src": "/goto/(.*)",
      "dest": "https://goto.casca.vercel.app/$1",
      "status": 200
    }
  ]
}

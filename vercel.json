{
  "version": 2,
  "rewrites": [
    {
      "source": "/(goto(?:/.*)?)",
      "destination": "https://goto.casca.vercel.app/$1"
    }
  ],
  "redirects": [
    {
      "source": "/goto/((?!favicon\\.ico)[^/]+)",
      "permanent": false,
      "destination": "https://goto.casca.vercel.app/api/redirect/$1"
    }
  ]
}

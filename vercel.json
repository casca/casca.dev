{
  "version": 2,
  "routes": [
    {
      "src": "/goto/index.html",
      "status": 302,
      "headers": { "Location": "/goto" },
      "continue": true
    },
    {
      "src": "/goto(/|.*)/index.html",
      "status": 302,
      "headers": { "Location": "/goto/foo/$1" },
      "continue": true
    },
    {
      "src": "/goto(.*)/",
      "status": 302,
      "headers": { "Location": "/goto$1" },
      "continue": true
    },
    {
      "src": "/goto/api/shorten",
      "status": 302,
      "headers": { "Location": "https://goto.casca.vercel.app/api/shorten" }
    },
    {
      "src": "/goto/api/stats",
      "status": 302,
      "headers": { "Location": "https://goto.casca.vercel.app/api/stats" }
    },
    {
      "src": "/goto/css/(.*)",
      "status": 302,
      "headers": { "Location": "https://goto.casca.vercel.app/css/$1" }
    },
    {
      "src": "/goto/img/(.*)",
      "status": 302,
      "headers": { "Location": "https://goto.casca.vercel.app/img/$1" }
    },
    {
      "src": "/goto/js/(.*)",
      "status": 302,
      "headers": { "Location": "https://goto.casca.vercel.app/js/$1" }
    },
    {
      "src": "/goto/favicon.ico",
      "dest": "https://goto.casca.vercel.app/favicon.ico",
      "status": 302
    },
    { "src": "/goto", "methods": ["GET"], "dest": "https://goto.casca.vercel.app/", "status": 200 },
    {
      "src": "/goto",
      "methods": ["POST"],
      "status": 307,
      "headers": { "Location": "https://goto.casca.vercel.app/api/date" }
    },
    {
      "src": "/goto/(?<id>[^/]+)",
      "status": 302,
      "headers": { "Location": "https://goto.casca.vercel.app/api/redirect/$id" }
    }
  ]
}
